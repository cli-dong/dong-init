<ul class="nav">
  {{#each folders}}
    <li class="folder{{#if active}} folder-active{{/if}}" data-folder="{{@index}}">
      <a class="iconfont iconfont-{{icon}}" href="/#/{{@index}}"><span>{{title}}</span></a>
      <ul class="routes">
        {{#each routes}}
          <li class="route{{#if active}} route-active{{/if}}">
            <a href="/#/{{route}}" data-route="{{route}}"><span>{{title}}</span></a>
          </li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>
